<% provide(:title, "Count Cells") %>
<%= render "layouts/header" %>
<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
      <%= @user.email %>
    </section>
  </aside>
</div>

<table class = "table"></br>
DNA UPLOADS
  <thead>
  <tr>
    <th>Name</th>
    <th> </th>
  </tr>
  </thead>

  <tbody>
  <% @dnas.each do |dna| %>
      <tr>
        <td><%= dna.name  %></td>
        <td><%= link_to "Download Image", dna.attachment_url %></td>

      </tr>


  <% end %>
  </tbody>

</table>
</br>
<table class = "table"></br>
  COMPARE CELL UPLOADS
  <thead>
  <tr>
    <th>Name</th>
    <th> </th>
  </tr>
  </thead>

  <tbody>
  <% @compares.each do |compare| %>
      <tr>
        <td><%= compare.name  %></td>
        <td><%= link_to "Download Image", compare.attachment_url %></td>
      </tr>


  <% end %>
  </tbody>

</table>
</br>
<table class = "table"></br>
  COUNT CELLS UPLOADS
  <thead>
  <tr>
    <th>Name</th>
    <th> </th>
  </tr>
  </thead>

  <tbody>
  <% @counts.each do |counter| %>
      <tr>
        <td><%= counter.name  %></td>
        <td><%= link_to "Download Image", counter.attachment_url %></td>
      </tr>


  <% end %>
  </tbody>

</table>